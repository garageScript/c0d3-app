    "express-session": "^1.17.1",
    "formik": "^2.2.8",
    "gitdiff-parser": "^0.2.2",
    "graphql": "^15.5.1",
    "graphql-request": "^3.5.0",
    "lodash": "^4.17.20",
    "markdown-to-jsx": "^7.1.3",
    "nanoid": "^3.1.25",
    "next": "^11.1.0",
    "next-connect": "^0.10.2",
    "nodemailer": "^6.6.3",
