{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "next -p $PORT",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts --ext .tsx",
    "autofix": "eslint . --ext .js --ext .ts --ext .tsx --fix && svgo public/*.svg",
    "storybook": "start-storybook -p 6006 -c .storybook -s ./public",
    "build-storybook": "build-storybook -c .storybook -s ./public",
    "test": "jest --coverage",
    "type-check": "tsc"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@types/yup": "^0.26.32",
    "@zeit/next-sass": "^1.0.1",
    "apollo-boost": "^0.4.7",
    "apollo-client": "^2.6.8",
    "apollo-link-http": "^1.5.16",
    "apollo-server-micro": "^2.12.0",
    "bcrypt": "^4.0.1",
    "bootstrap": "^4.4.1",
    "connect-session-sequelize": "^6.1.1",
    "express-session": "^1.17.0",
    "formik": "^2.1.4",
    "graphql": "^14.6.0",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-unfetch": "3.0.0",
    "lodash": "^4.17.15",
    "markdown-to-jsx": "^6.11.1",
    "nanoid": "^3.1.3",
    "next": "^9.3.2",
    "next-connect": "^0.6.3",
    "next-with-apollo": "^5.0.0",
    "node-sass": "^4.13.1",
    "pg": "^8.0.2",
    "pg-hstore": "^2.3.3",
    "react": "^16.12.0",
    "react-diff-viewer": "^3.0.2",
    "react-dom": "^16.12.0",
    "react-feather": "^2.0.3",
    "sequelize": "^5.21.6",
    "swr": "^0.1.18",
    "yup": "^0.28.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@storybook/addon-actions": "^5.3.13",
    "@storybook/addon-storyshots": "^5.3.13",
    "@storybook/addon-viewport": "^5.3.13",
    "@storybook/react": "^5.3.13",
    "@testing-library/react": "^9.4.0",
    "@types/bcrypt": "^3.0.0",
    "@types/express-session": "^1.17.0",
    "@types/lodash": "^4.14.150",
    "@types/markdown-to-jsx": "^6.11.0",
    "@types/node": "^12.12.28",
    "@types/node-fetch": "^2.5.4",
    "@types/react": "^16.9.21",
    "@types/react-dom": "^16.9.4",
    "@types/styled-jsx": "^2.2.8",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "babel-preset-react-app": "^9.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "svgo": "^1.3.2",
    "typescript": "3.7.3"
  },
  "license": "ISC",
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test && npm run type-check",
      "pre-push": "npm run lint && npm run test && npm run type-check"
    }
  }
}
