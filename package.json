{
  "name": "with-typescript",
  "version": "1.0.0",
  "license": "ISC",
  "scripts": {
    "db:init": "prisma migrate reset",
    "db:seed": "prisma db seed --preview-feature",
    "db:up": "docker-compose --file docker/docker-compose.yml up --detach postgres",
    "db:down": "docker-compose --file docker/docker-compose.yml down",
    "dev": "next --port $PORT",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js --ext .ts --ext .tsx --ext .mdx",
    "lint:fix": "yarn lint --fix && prettier --write ./scss/",
    "autofix": "yarn lint:fix && svgo --folder=public --recursive && svgo --folder=assets --recursive",
    "storybook": "start-storybook -p $STORY_PORT -c .storybook -s ./public",
    "build-storybook": "build-storybook -c .storybook -s ./public",
    "test": "jest --coverage",
    "ts-node": "ts-node --compiler-options {\\\"module\\\":\\\"CommonJS\\\"}",
    "type-check": "tsc",
    "generate": "graphql-codegen --config codegen.yml",
    "postinstall": "node scripts/prismaUtils.js setup && prisma generate",
    "prepare": "husky install",
    "vercel-build": "node scripts/prismaUtils.js baseline migrate && next build"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mdx}": "eslint --fix",
    "*.{json,md,scss}": "prettier --write",
    "*.svg": "svgo --multipass",
    "prisma/schema.prisma": "npx prisma format"
  },
  "dependencies": {
    "@apollo/client": "^3.3.16",
    "@prisma/client": "^2.21.2",
    "@quixo3/prisma-session-store": "^3.0.1",
    "@sentry/browser": "^6.3.5",
    "apollo-server-micro": "^2.24.0",
    "apollo3-cache-persist": "^0.9.1",
    "bcrypt": "5.0.0",
    "bootstrap": "^4.6.0",
    "dayjs": "^1.10.4",
    "express-session": "^1.17.1",
    "formik": "^2.2.6",
    "gitdiff-parser": "^0.2.2",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "lodash": "^4.17.20",
    "mailgun-js": "^0.22.0",
    "markdown-to-jsx": "^7.1.1",
    "nanoid": "^3.1.20",
    "next": "^10.2.0",
    "next-connect": "^0.10.0",
    "posthog-js": "^1.9.7",
    "prism-react-renderer": "^1.2.0",
    "prismjs": "^1.23.0",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.3",
    "react-diff-viewer": "^3.1.1",
    "react-dom": "^17.0.1",
    "react-feather": "^2.0.9",
    "sass": "^1.32.12",
    "winston": "^3.3.3",
    "winston-transport-sentry-node": "^1.0.2",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.0",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.12.13",
    "@graphql-codegen/cli": "^1.21.4",
    "@graphql-codegen/typescript": "^1.21.0",
    "@graphql-codegen/typescript-operations": "^1.17.16",
    "@graphql-codegen/typescript-react-apollo": "^2.2.4",
    "@graphql-codegen/typescript-resolvers": "^1.19.1",
    "@mdx-js/loader": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@next/mdx": "^10.2.0",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-storyshots": "^6.1.17",
    "@storybook/addon-storysource": "^6.2.9",
    "@storybook/addon-viewport": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.7.0",
    "@types/bcrypt": "^3.0.1",
    "@types/express-session": "^1.17.3",
    "@types/lodash": "^4.14.168",
    "@types/mailgun-js": "^0.22.11",
    "@types/mdx-js__react": "^1.5.3",
    "@types/node": "^15.0.1",
    "@types/node-fetch": "^2.5.10",
    "@types/prismjs": "^1.16.5",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.0",
    "@types/styled-jsx": "^2.2.8",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-prismjs": "^2.0.1",
    "babel-preset-react-app": "^10.0.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-mdx": "^1.13.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "prisma": "^2.21.2",
    "react-test-renderer": "^17.0.1",
    "remark-gfm": "^1.0.0",
    "remark-slug": "^6.0.0",
    "remark-toc": "^7.2.0",
    "sass-loader": "^10.0.1",
    "svgo": "^2.3.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
