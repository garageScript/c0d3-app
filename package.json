{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "next -p $PORT",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js --ext .ts --ext .tsx",
    "autofix": "eslint . --ext .js --ext .ts --ext .tsx --fix && svgo --folder=public --recursive && svgo --folder=assets --recursive",
    "storybook": "start-storybook -p $STORY_PORT -c .storybook -s ./public",
    "build-storybook": "build-storybook -c .storybook -s ./public",
    "test": "jest --coverage",
    "type-check": "tsc",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.3.11",
    "@sentry/browser": "^5.30.0",
    "@zeit/next-sass": "^1.0.1",
    "apollo-server-micro": "^2.20.0",
    "bcrypt": "^5.0.1",
    "bootstrap": "^4.6.0",
    "connect-session-sequelize": "^6.1.1",
    "dayjs": "^1.10.4",
    "express-session": "^1.17.1",
    "formik": "^2.2.6",
    "gitdiff-parser": "^0.2.2",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "lodash": "^4.17.20",
    "mailgun-js": "^0.22.0",
    "markdown-to-jsx": "^7.1.1",
    "nanoid": "^3.1.20",
    "next": "^10.0.6",
    "next-connect": "^0.10.0",
    "next-with-apollo": "^5.1.1",
    "node-sass": "^5.0.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "posthog-js": "^1.9.1",
    "prismjs": "^1.23.0",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.3",
    "react-diff-viewer": "^3.1.1",
    "react-dom": "^17.0.1",
    "react-feather": "^2.0.9",
    "sequelize": "^5.22.3",
    "swr": "^0.4.2",
    "winston": "^3.3.3",
    "winston-transport-sentry-node": "^0.7.0",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@graphql-codegen/cli": "^1.21.2",
    "@graphql-codegen/typescript": "^1.21.0",
    "@graphql-codegen/typescript-operations": "^1.17.14",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "@graphql-codegen/typescript-resolvers": "^1.18.2",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-storyshots": "^6.1.17",
    "@storybook/addon-storysource": "^6.1.21",
    "@storybook/addon-viewport": "^6.1.17",
    "@storybook/react": "^6.1.17",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.7.0",
    "@types/bcrypt": "^3.0.0",
    "@types/express-session": "^1.17.3",
    "@types/lodash": "^4.14.168",
    "@types/mailgun-js": "^0.22.11",
    "@types/node": "^14.14.32",
    "@types/node-fetch": "^2.5.8",
    "@types/prismjs": "^1.16.2",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/styled-jsx": "^2.2.8",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.17.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-inline-react-svg": "^1.1.2",
    "babel-plugin-prismjs": "^2.0.1",
    "babel-preset-react-app": "^10.0.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^4.3.7",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.1",
    "svgo": "^1.3.2",
    "typescript": "^4.1.5"
  },
  "license": "ISC",
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "coveragePathIgnorePatterns": [
      "stories",
      "testUtil",
      "graphql/index.tsx"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test && npm run type-check",
      "pre-push": "npm run lint && npm run test && npm run type-check"
    }
  }
}
