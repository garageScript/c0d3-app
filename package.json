    "apollo-server-micro": "^2.24.1",
    "apollo3-cache-persist": "^0.9.1",
    "bcrypt": "5.0.0",
    "bootstrap": "^4.6.0",
    "dayjs": "^1.10.4",
    "express-session": "^1.17.1",
    "formik": "^2.2.8",
    "gitdiff-parser": "^0.2.2",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "isomorphic-unfetch": "^3.1.0",
    "lodash": "^4.17.20",
    "mailgun-js": "^0.22.0",
    "markdown-to-jsx": "^7.1.1",
    "nanoid": "^3.1.23",
    "next": "^10.2.0",
    "next-connect": "^0.10.0",
    "posthog-js": "^1.10.0",
    "prism-react-renderer": "^1.2.0",
    "prismjs": "^1.23.0",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.3",
