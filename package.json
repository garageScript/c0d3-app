{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "next -p $PORT",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js --ext .ts --ext .tsx",
    "autofix": "eslint . --ext .js --ext .ts --ext .tsx --fix && svgo public/*.svg && svgo assets/**/*.svg",
    "storybook": "start-storybook -p $STORY_PORT -c .storybook -s ./public",
    "build-storybook": "build-storybook -c .storybook -s ./public",
    "test": "jest --coverage",
    "type-check": "tsc",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.3.7",
    "@sentry/browser": "^5",
    "@zeit/next-sass": "^1",
    "apollo-server-micro": "^2",
    "bcrypt": "^5",
    "bootstrap": "^4",
    "connect-session-sequelize": "^6",
    "dayjs": "^1",
    "express-session": "^1",
    "formik": "^2",
    "gitdiff-parser": "^0",
    "graphql": "^15",
    "graphql-request": "^3",
    "identity-obj-proxy": "^3",
    "isomorphic-unfetch": "^3",
    "lodash": "^4",
    "mailgun-js": "^0",
    "markdown-to-jsx": "^7",
    "nanoid": "^3",
    "next": "^10",
    "next-connect": "^0",
    "next-with-apollo": "^5",
    "node-sass": "^5",
    "pg": "^8",
    "pg-hstore": "^2",
    "posthog-js": "^1",
    "prismjs": "^1",
    "react": "^17",
    "react-bootstrap": "^1",
    "react-diff-viewer": "^3",
    "react-dom": "^17",
    "react-feather": "^2",
    "sequelize": "^5",
    "swr": "^0",
    "winston": "^3",
    "winston-transport-sentry-node": "^0",
    "yup": "^0"
  },
  "devDependencies": {
    "@babel/core": "^7",
    "@babel/preset-env": "^7",
    "@babel/preset-react": "^7",
    "@babel/preset-typescript": "^7",
    "@graphql-codegen/cli": "^1",
    "@graphql-codegen/typescript": "^1",
    "@graphql-codegen/typescript-operations": "^1",
    "@graphql-codegen/typescript-react-apollo": "^2",
    "@graphql-codegen/typescript-resolvers": "^1",
    "@storybook/addon-actions": "^6",
    "@storybook/addon-storyshots": "^6",
    "@storybook/addon-storysource": "^6",
    "@storybook/addon-viewport": "^6",
    "@storybook/react": "^6",
    "@testing-library/react": "^11",
    "@testing-library/user-event": "^12",
    "@types/bcrypt": "^3",
    "@types/express-session": "^1",
    "@types/lodash": "^4",
    "@types/mailgun-js": "^0",
    "@types/node": "^14",
    "@types/node-fetch": "^2",
    "@types/prismjs": "^1",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@types/styled-jsx": "^2",
    "@typescript-eslint/eslint-plugin": "^4",
    "@typescript-eslint/parser": "^4",
    "babel-jest": "^26",
    "babel-loader": "^8",
    "babel-plugin-inline-react-svg": "^1",
    "babel-plugin-prismjs": "^2",
    "babel-preset-react-app": "^10",
    "eslint": "^7",
    "eslint-config-prettier": "^7",
    "eslint-plugin-prettier": "^3",
    "eslint-plugin-react": "^7",
    "husky": "^4",
    "jest": "^26",
    "prettier": "^2",
    "react-test-renderer": "^17",
    "svgo": "^1",
    "typescript": "^4"
  },
  "license": "ISC",
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "coveragePathIgnorePatterns": [
      "stories",
      "testUtil",
      "graphql/index.tsx"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test && npm run type-check",
      "pre-push": "npm run lint && npm run test && npm run type-check"
    }
  }
}
